{
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "img/voices-icon-16x16.png",
      "48": "img/voices-icon-48x48.png",
      "128": "img/voices-icon-128x128.png"
    }
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["img/gvaa-icon.png"],
      "matches": ["https://www.voices.com/*"]
    }
  ]
}
